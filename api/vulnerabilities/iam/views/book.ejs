<%- include('partials/header') %>
<div class="container">
  <h1>Books</h1>
  <table class="table">
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Author</th>
      </tr>
    </thead>
    <tbody>
      <% books.forEach(function(book) { %>
        <tr>
          <td><%= book.id %></td>
          <td><%= book.title %></td>
          <td><%= book.author %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>
<%- include('partials/footer') %>
